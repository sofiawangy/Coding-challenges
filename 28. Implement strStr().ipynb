    "def strStr(haystack, needle):\n",
    "    if needle == \"\":\n",
    "        return 0\n",
    "    \n",
    "    if needle not in haystack:\n",
    "        return -1\n",
    "    \n",
    "    for i in range(0, len(haystack)):\n",
    "        if haystack[i:i+len(needle)] == needle:\n",
    "            return i"
   ]
