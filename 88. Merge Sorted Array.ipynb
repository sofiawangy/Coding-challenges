    "m = 0\n",
    "n = 1\n",
    "\n",
    "nums1 = [0]\n",
    "nums2 = [1]\n",
    "\n",
    "while m > 0 and n >0:\n",
    "    if nums1[m-1] > nums2[n-1]:\n",
    "        nums1[m+n-1] = nums1[m-1]\n",
    "        m-=1\n",
    "    else:\n",
    "        nums1[m+n-1] = nums2[n-1]\n",
    "        n-=1\n",
    "\n",
    "nums1[:n] = nums2[:n]\n",
    "nums1"
   ]
