   "source": [
    "def sqrt(x):\n",
    "    hi = x\n",
    "    lo = 0\n",
    "    while lo <= hi:\n",
    "        mid = (hi+lo)//2\n",
    "\n",
    "        if mid*mid > x:\n",
    "            hi = mid -1\n",
    "        elif mid*mid < x:\n",
    "            lo = mid +1\n",
    "        else:\n",
    "            return mid\n",
    "    return hi"
   ]
