    "def climbStairs(n):\n",
    "\n",
    "    if n == 1:\n",
    "        return 1\n",
    "\n",
    "    if n == 2:\n",
    "        return 2\n",
    "\n",
    "    first = 1\n",
    "    second = 2\n",
    "\n",
    "    for _ in range(3,n+1):\n",
    "        \n",
    "        second, first = first + second, second\n",
    "\n",
    "    return second"
   ]
