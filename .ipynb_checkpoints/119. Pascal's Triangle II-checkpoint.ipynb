    "def getRow(rowIndex):\n",
    "    \"\"\"\n",
    "    :type rowIndex: int\n",
    "    :rtype: List[int]\n",
    "    \"\"\"\n",
    "    pascal = [1]*(rowIndex + 1)\n",
    "    for i in range(2,rowIndex+1):\n",
    "        for j in range(i-1,0,-1):\n",
    "            pascal[j] += pascal[j-1]\n",
    "    return pascal"
   ]
