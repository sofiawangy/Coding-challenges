   "source": [
    "def rob(nums):\n",
    "    prev = 0\n",
    "    curr = 0\n",
    "    \n",
    "    for i in nums:\n",
    "        temp = curr\n",
    "        curr = max(prev + i, curr)\n",
    "        prev = temp\n",
    "    \n",
    "    return curr"
   ]
  },
