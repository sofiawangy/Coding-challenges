    "def singleNumber(nums):\n",
    "    seen = set()\n",
    "    while nums:\n",
    "        if nums[0] not in seen:\n",
    "            seen.add(nums[0])\n",
    "\n",
    "        else:\n",
    "            seen.remove(nums[0])\n",
    "        \n",
    "        nums.pop(0)\n",
    "    return seen.pop()"
   ]
